@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .


<http://example.com/lucianodato#me>
  a foaf:Person ;
  foaf:name "Luciano Dato" ;
  foaf:homepage <https://github.com/lucianodato> ;
  foaf:mbox <mailto:lucianodato@gmail.com> .


<https://github.com/lucianodato/noise-repellent>
  a lv2:Plugin, lv2:SpectralPlugin, lv2:UtilityPlugin, doap:Project ;
  doap:maintainer <http://example.com/lucianodato#me> ;
  doap:license <https://opensource.org/licenses/LGPL-3.0> ;
  doap:name "Noise Repellent",
    "Repelente de Ruido"@es ,
    "Répulseur de bruit"@fr ;
  @VERSION@
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:extensionData state:interface ;
  lv2:requiredFeature urid:map ;

  lv2:port [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "reduction" ;
    lv2:name "Reduction Amount",
      "Cantidad de Reduccion"@es ,
      "Quantité de réduction"@fr ;
    lv2:minimum 0.0 ;
    lv2:maximum 48.0 ;
    lv2:default 5.0 ;
    units:unit units:db ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 1 ;
    lv2:symbol "offset" ;
    lv2:name "Threshold offset",
      "Compensacion de Umbrales"@es ;
    lv2:minimum -24.0 ;
    lv2:maximum 24.0 ;
    lv2:default 0.0 ;
    units:unit units:db ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 2 ;
    lv2:symbol "release" ;
    lv2:name "Release",
      "Relevo"@es ;
    lv2:minimum 1.0 ;
    lv2:maximum 1000.0 ;
    lv2:default 30.0 ;
    units:unit units:ms ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 3 ;
    lv2:symbol "smoothing" ;
    lv2:name "Smoothing",
      "Suavizado"@es ,
      "Lissage"@fr ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
    lv2:default 0.0 ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 4 ;
    lv2:symbol "artifact_control" ;
    lv2:name "Artifact Control",
      "Control de Artefactos"@es ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
    lv2:default 0.0 ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 5 ;
    lv2:symbol "whitening" ;
    lv2:name "Residual whitening",
      "Blanqueo de residuo"@es ,
      "Blanchissement du bruit"@fr ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
    lv2:default 0.0 ;
  ], [
    a lv2:InputPort,
    lv2:ControlPort ;
    lv2:index 6 ;
    lv2:symbol "makeup";
    lv2:name "Makeup Gain",
      "Compensacion de Ganancia"@es ,
      "Compensation de gain"@fr ;
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 20.0 ;
    units:unit units:db ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 7 ;
    lv2:symbol "noise_capture" ;
    lv2:name "Capture Noise Print",
      "Capturar Perfil de Ruido"@es ,
      "Capturer l'empreinte de bruit"@fr ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 8 ;
    lv2:symbol "adaptive_noise" ;
    lv2:name "Adaptive Noise",
          "Adaptar Ruido"@es ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:InputPort,
    lv2:ControlPort ;
    lv2:index 9 ;
    lv2:symbol "reset_noise_print" ;
    lv2:name "Reset Noise Print",
      "Reiniciar Perfil de Ruido"@es ,
      "Réinitialiser l'empreinte de bruit"@fr ;
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:portProperty lv2:integer, lv2:toggled, pprop:trigger ;
  ], [
    a lv2:InputPort,
    lv2:ControlPort ;
    lv2:index 10 ;
    lv2:symbol "noise_listen" ;
    lv2:name "Noise Listen",
      "Escuchar solo Ruido"@es ,
      "Écoute du bruit"@fr ;
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 11 ;
    lv2:symbol "tappering" ;
    lv2:name "HF residual emphasis",
            "Enfasis HF Residual"@es ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:InputPort, lv2:ControlPort ;
    lv2:index 12 ;
    lv2:name "Enable" ;
    lv2:symbol "enable" ;
    lv2:default 1.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
    lv2:portProperty lv2:toggled, pprop:notOnGUI;
    lv2:designation <http://ardour.org/lv2/processing#enable>;
  ], [
    a lv2:OutputPort,
      lv2:ControlPort ;
    lv2:name "latency" ;
    lv2:index 13 ;
    lv2:symbol "latency" ;
    lv2:minimum 0 ;
    lv2:maximum 8196 ;
    lv2:portProperty lv2:reportsLatency, lv2:integer, pprop:notOnGUI ;
    units:unit units:frame ;
  ], [
    a lv2:AudioPort,
      lv2:InputPort ;
    lv2:index 14 ;
    lv2:symbol "input" ;
    lv2:name "Input" ;
  ], [
    a lv2:AudioPort,
      lv2:OutputPort ;
    lv2:index 15 ;
    lv2:symbol "output" ;
    lv2:name "Output" ;
  ];
  rdfs:comment "Instructions: Select a section of noise only in your track and loop it. Turn on noise capture for one second or two to learn the noise profile and then turn it off. Then you can now adjust the reduction.",
   "Instrucciones: Seleccionar una seccion de la pista que solo contenga ruido y reproducirla ciclicamente. Encender la captura del perfil de ruido por unos segundos para capturar el perfil de ruido y luego apagarlo. Luego se puede ajustar la reduccion."@es ,
   "Instructions : sélectionnez une portion de bruit seul dans votre piste et bouclez-la. Activez la capture de l'empreinte pendant une seconde ou deux pour apprendre le profil du bruit, puis désactivez-la. Vous pouvez alors ajuster la réduction."@fr ;
.
