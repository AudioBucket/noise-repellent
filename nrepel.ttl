@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix param: <http://lv2plug.in/ns/ext/parameters#>.
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/lucianodato/noise-repellent>
  a lv2:Plugin;
  a lv2:SpectralPlugin;
  a lv2:UtilityPlugin;

  lv2:binary <nrepel.so>;
  doap:name "Noise Repellent";
  doap:maintainer [
     foaf:name "Luciano Dato";
     foaf:homepage <https://github.com/lucianodato/noise-repellent>;
     foaf:mbox <mailto:lucianodato@gmail.com>;
  ];

  doap:license <https://opensource.org/licenses/LGPL-3.0>;
  lv2:optionalFeature pprops:supportsStrictBounds ;

  lv2:port [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "capture";
    lv2:name "Capture noise print";
    lv2:minimum 0.0 ;
    lv2:maximum 2.0 ;
    lv2:default 0.0 ;
    lv2:portProperty lv2:enumeration, lv2:integer;
    lv2:scalePoint [
    	rdfs:label "OFF";
		  rdf:value 0
		] , [
		  rdfs:label "ON";
		  rdf:value 1
		] , [
		  rdfs:label "ADAPTIVE";
		  rdf:value 2
		]
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:symbol "noisesmoothing";
    lv2:name "Noise Time Smoothing";
    lv2:minimum 0.0 ;
    lv2:maximum 2.0 ;
    lv2:default 0.0 ;
    lv2:portProperty lv2:enumeration, lv2:integer;
    lv2:scalePoint [
    	rdfs:label "Max";
		  rdf:value 0
		] , [
		  rdfs:label "Geometric Mean";
		  rdf:value 1
		] , [
		  rdfs:label "Average";
		  rdf:value 2
		]
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "denoisemethod";
    lv2:name "Denoise algorithm";
    lv2:minimum 0.0 ;
    lv2:maximum 2.0 ;
    lv2:default 2.0 ;
    lv2:portProperty lv2:enumeration, lv2:integer;
    lv2:scalePoint [
    	rdfs:label "Wiener";
		  rdf:value 0
		] , [
		  rdfs:label "Power Subtraction";
		  rdf:value 1
		] , [
		  rdfs:label "Ephraim-Mallat";
		  rdf:value 2
		]
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "amountreduc";
    lv2:name "Percentage of reduction";
    lv2:portProperty pprops:hasStrictBounds;
    lv2:minimum 0.0;
    lv2:maximum 100.0;
    lv2:default 0.0;
    units:unit units:pc;
  ], [
    a lv2:OutputPort, lv2:ControlPort ;
    lv2:name "latency";
    lv2:index 4;
    lv2:symbol "latency";
    lv2:minimum 0;
		lv2:maximum 8196;
		lv2:portProperty lv2:reportsLatency, lv2:integer;
		units:unit units:frame;
  ], [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "reset_noise_print" ;
		lv2:name "Reset noise print" ;
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1;
		lv2:portProperty lv2:toggled, lv2:integer;
	], [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 6;
    lv2:symbol "input";
    lv2:name "Input";
  ], [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 7;
    lv2:symbol "output";
    lv2:name "Output";
  ];
